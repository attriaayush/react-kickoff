{
  "name": "React-KickOff",
  "version": "1.0.0",
  "description": "React Start-Up kit/Boiler plate code.",
  "scripts": {
    "start-message": "babel-node tools/message.js",
    "prestart": "npm run start-message",
    "start": "concurrently -k -r -s first \"npm run test:watch\" \"npm run lint:watch\"",
    "open:src": "babel-node tools/server.js",
    "lint": "esw webpack.config.* src tools --color",
    "lint:watch": "npm run lint -- --watch",
    "test": "jest",
    "test:CI": "babel-node tools/testConfigure.js",
    "test:cover": "npm run test -- --coverage ",
    "test:cover:CI": "npm run test:CI -- --coverage && node_modules/coveralls/bin/coveralls.js",
    "test:watch": "jest --watchAll"
  },
  "author": "Aayush Attri",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.6.0",
    "babel-polyfill": "^6.8.0",
    "font-awesome": "^4.7.0",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "react-redux": "5.0.7",
    "react-router": "^4.3.1",
    "react-router-redux": "^4.0.8",
    "redux": "^4.0.0",
    "toastr": "^2.1.4",
    "redux-thunk": "2.3.0"
  },
  "devDependencies": {
    "@babel/cli": "7.0.0",
    "@babel/core": "7.0.0",
    "@babel/node": "7.0.0",
    "@babel/plugin-proposal-class-properties": "7.0.0",
    "@babel/plugin-transform-react-constant-elements": "7.0.0",
    "@babel/polyfill": "7.0.0",
    "@babel/preset-env": "7.0.0",
    "@babel/preset-react": "7.0.0",
    "autoprefixer": "9.1.3",
    "coveralls": "3.0.2",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "9.0.0",
    "babel-jest": "23.4.2",
    "babel-loader": "8.0.0",
    "babel-plugin-transform-react-remove-prop-types": "0.4.15",
    "sass-loader": "^7.1.0",
    "bootstrap": "4.0.0",
    "less": "^3.8.1",
    "popper.js": "^1.14.4",
    "jquery": "^3.3.1",
    "react-hot-loader": "4.3.5",
    "jest": "23.5.0",
    "jest-cli": "23.5.0",
    "cheerio": "^0.22.0",
    "chalk": "2.4.1",
    "concurrently": "4.0.1",
    "compression": "^1.6.1",
    "cross-env": "^1.0.7",
    "css-loader": "^1.0.0",
    "enzyme": "^3.5.0",
    "enzyme-adapter-react-16": "1.3.1",
    "eslint": "5.4.0",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-react": "7.11.1",
    "eslint-watch": "4.0.2",
    "eventsource-polyfill": "^0.9.6",
    "expect": "^1.19.0",
    "exports-loader": "^0.7.0",
    "express": "^4.13.4",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "file-loader": "2.0.0",
    "font-awesome-loader": "^1.0.2",
    "imports-loader": "^0.8.0",
    "jsdom": "^8.5.0",
    "less-loader": "^4.1.0",
    "nock": "^9.6.1",
    "node-sass": "^4.9.3",
    "npm-run-all": "^1.8.0",
    "open": "^0.0.5",
    "path": "^0.12.7",
    "postcss-loader": "^3.0.0",
    "precss": "^3.1.2",
    "redux-immutable-state-invariant": "^1.2.3",
    "redux-mock-store": "^1.0.2",
    "rimraf": "^2.5.2",
    "style-loader": "^0.13.1",
    "tether": "^1.4.4",
    "url-loader": "^1.1.1",
    "webpack": "^4.17.1",
    "webpack-dev-middleware": "^3.2.0",
    "webpack-hot-middleware": "^2.23.0"
  },
  "jest": {
    "verbose": true
  },
  "babel": {
    "env": {
      "development": {
        "presets": [
          "@babel/preset-env",
          "@babel/preset-react"
        ],
        "plugins": [
          "@babel/plugin-proposal-class-properties",
          "react-hot-loader/babel"
        ]
      }
    }
  },
  "eslintConfig": {
    "root": true,
    "extends": [
      "eslint:recommended",
      "plugin:import/errors",
      "plugin:import/warnings",
      "plugin:react/recommended"
    ],
    "plugins": [
      "react"
    ],
    "parser": "babel-eslint",
    "parserOptions": {
      "ecmaVersion": 6,
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true,
        "experimentalObjectRestSpread": true
      }
    },
    "env": {
      "es6": true,
      "browser": true,
      "node": true,
      "jquery": true,
      "jest": true
    }
  },
  "repository": {}
}

